{
  "name": "ronas-it-nx-generators",
  "displayName": "@ronas-it/nx-generators",
  "description": "@ronas-it/nx-generators VS Code extention",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:/nx.json"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ronas-it-nx-generators.react-component",
        "title": "Generate React component"
      },
      {
        "command": "ronas-it-nx-generators.react-lib",
        "title": "Generate React library"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "submenu": "ronas-it-nx-generators.submenus.generators",
          "group": "ronas-it-nx-generators.groups.generators"
        }
      ],
      "ronas-it-nx-generators.submenus.generators": [
        {
          "command": "ronas-it-nx-generators.react-component"
        },
        {
          "command": "ronas-it-nx-generators.react-lib"
        }
      ]
    },
    "submenus": [
      {
        "id": "ronas-it-nx-generators.submenus.generators",
        "label": "Ronas IT/NX generators"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.15",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.24.2",
    "eslint": "^9.19.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
